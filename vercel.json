{
    "version": 2,
    "build": {
        "env": {
            "VITE_SOME_VARIABLE": "@vite_some_variable"
        }
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}